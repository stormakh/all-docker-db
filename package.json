{
  "name": "db-containers",
  "version": "1.0.0",
  "description": "Docker-compose setup for various databases",
  "scripts": {
    "start:all": "node run.js all start",
    "stop:all": "node run.js all stop",
    "start:postgres": "node run.js postgres start",
    "stop:postgres": "node run.js postgres stop",
    "start:mongodb": "node run.js mongodb start",
    "stop:mongodb": "node run.js mongodb stop",
    "start:redis": "node run.js redis start",
    "stop:redis": "node run.js redis stop",
    "start:neo4j": "node run.js neo4j start",
    "stop:neo4j": "node run.js neo4j stop",
    "start:cassandra": "node run.js cassandra start",
    "stop:cassandra": "node run.js cassandra stop",
    "start:objectdb": "node run.js objectdb start",
    "stop:objectdb": "node run.js objectdb stop",
    "cli:postgres": "node run.js postgres cli",
    "cli:mongodb": "node run.js mongodb cli",
    "cli:redis": "node run.js redis cli",
    "cli:neo4j": "node run.js neo4j cli",
    "cli:cassandra": "node run.js cassandra cli",
    "cli:objectdb": "node run.js objectdb cli",
    "delete:all": "docker-compose down -v --remove-orphans"
  },
  "author": "Santiago Tormakh",
  "license": "MIT"
}
